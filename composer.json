{
  "name": "ltiollier-webid/laravel-couchdb",
  "description": "A CouchDB v2.0.0 based Eloquent model and Query builder for Laravel",
  "authors": [
    {
      "name": "Robson Nascimento",
      "email": "robsonvnasc@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^7.3",
    "illuminate/database": "7.*",
    "illuminate/support": "7.*",
    "illuminate/container": "7.*",
    "illuminate/events": "7.*",
    "doctrine/couchdb": "^2.0.0-alpha1",
    "adbario/php-dot-notation": "^1.2"
  },
  "require-dev": {
    "laravel/passport": "^9.3.2",
    "phpunit/phpunit": "~6.0",
    "mockery/mockery": "^0.9"
  },
  "autoload": {
    "psr-4": {
      "Robsonvn\\CouchDB\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php",
      "tests/models",
      "tests/seeds"
    ]
  }
}
